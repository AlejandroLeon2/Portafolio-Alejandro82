---

import SeccionesTecnologias from "./SeccionesTecnologias.astro";
const { techStack } = Astro.props;
---

<div class="container mx-auto px-4">
  <ul class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-4 md:gap-5">
    {techStack.map((category) => <SeccionesTecnologias {...category} />)}
  </ul>
</div>
